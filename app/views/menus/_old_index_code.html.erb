<div class="col-md-3 col-xs-6">
      <div class="filterbox">
      <h2 class="filter">Sort by <span class="glyphicon glyphicon-filter filtericon"></span></h2>
        <div class="browse">
          <h3 id="filterheader">Food type</h3>
          <ul id="checkboxes">
            <li class="checkbox"><input type="checkbox" id="a"><label for="a" class="checkboxname">Bakery</label></li>
            <li class="checkbox"><input type="checkbox" id="b"><label for="b" class="checkboxname">BBQ</label></li>
            <li class="checkbox"><input type="checkbox" id="c"><label for="c" class="checkboxname">Brazilian</label></li>
            <li class="checkbox"><input type="checkbox" id="d"><label for="d" class="checkboxname">Breakfast</label></li>
            <li class="checkbox"><input type="checkbox" id="e"><label for="e" class="checkboxname">Burgers</label></li>
            <li class="checkbox"><input type="checkbox" id="f"><label for="f" class="checkboxname">Chinese</label></li>
            <li class="checkbox"><input type="checkbox" id="g"><label for="g" class="checkboxname">Coffee/Tea</label></li>
            <li class="checkbox"><input type="checkbox" id="h"><label for="h" class="checkboxname">Desserts</label></li>
            <li class="checkbox"><input type="checkbox" id="i"><label for="i" class="checkboxname">Greek</label></li>
            <li class="checkbox"><input type="checkbox" id="j"><label for="j" class="checkboxname">Hot Dogs/Sausages</label></li>
            <li class="checkbox"><input type="checkbox" id="k"><label for="k" class="checkboxname">Indian</label></li>
            <li class="checkbox"><input type="checkbox" id="l"><label for="l" class="checkboxname">Italian</label></li>
            <li class="checkbox"><input type="checkbox" id="m"><label for="m" class="checkboxname">Japanese</label></li>
            <li class="checkbox"><input type="checkbox" id="n"><label for="n" class="checkboxname">Mexican</label></li>
            <li class="checkbox"><input type="checkbox" id="o"><label for="o" class="checkboxname">Middle Eastern</label></li>
            <li class="checkbox"><input type="checkbox" id="p"><label for="p" class="checkboxname">Noodles</label></li>
            <li class="checkbox"><input type="checkbox" id="q"><label for="q" class="checkboxname">Pizza</label></li>
            <li class="checkbox"><input type="checkbox" id="r"><label for="r" class="checkboxname">Salad</label></li>
            <li class="checkbox"><input type="checkbox" id="s"><label for="s" class="checkboxname">Sandwiches/Subs/Wraps</label></li>
            <li class="checkbox"><input type="checkbox" id="t"><label for="t" class="checkboxname">Seafood</label></li>
            <li class="checkbox"><input type="checkbox" id="u"><label for="u" class="checkboxname">Steakhouse</label></li>
            <li class="checkbox"><input type="checkbox" id="v"><label for="v" class="checkboxname">Tapas</label></li>
            <li class="checkbox"><input type="checkbox" id="w"><label for="w" class="checkboxname">Thai</label></li>
            <li class="checkbox"><input type="checkbox" id="x"><label for="x" class="checkboxname">Vegetarian</label></li>
          </ul> 
        </div>
        </div><!--end filterbox-->
      </div><!--end col-md-3-->
    <div class="col-md-9 col-xs-12">
          <div class="nearyoubox">
          <h2 class="nearyou">Meals near you <span class="glyphicon glyphicon-map-marker mapmarker"></span></h2>
          </div>
      <div class="listtable">
        <div class="col-md-10 col-md-offset-1">
      <% @menus.each do |menu| %>
          <div class="listitemsall">
           <div class="listlineitem">
            <span class="glyphicon glyphicon-heart"></span>
           <span class="listofitems"><p class="listitem list-name" id="listitemname"><%= menu.item %></p>
           <p class="listitem list-calories listitemother">Calories: <span id="calories"><%= menu.calories %></span></p>
           <p class="listitem list-fat listitemother">Fat (g): <%= menu.fat %></p>
           <p class="listitem list-category listitemother"><%= menu.category %></p>
           <p class="listitem list-category listitemother"><%= menu.restaurant %></p></div>
          </div><!--end listitemsall-->
        <% end %>
    </div>
      </div><!--end listtable-->
         <% if @menus.empty? %> 
         <p>No Item Found</p>
        <% end %>
    </div><!--end col-md-9-->
  </div><!--end row-->
</div>
      <div class="row">
      <div id="foodpagefooter">